<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.domain.Student" table="Stu">
	 <id name="sid" column="id">
	  <generator class="native"/>
	 </id>
	<property name="sname" column="namee"></property>
	<property name="sage" column="agee"></property>
	<property name="ssex" column="sexe"></property>
	<property name="sadd" column="adde"></property>
	<property name="sphone" column="aphonee"></property>
 </class>
</hibernate-mapping>